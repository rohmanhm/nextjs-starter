{
  "name": "nextjs-starter",
  "version": "0.0.0",
  "private": true,
  "author": "Rohman Habib M <mhrohman@live.com>",
  "license": "ISC",
  "scripts": {
    "dev": "next",
    "start": "next start",
    "export": "next export",
